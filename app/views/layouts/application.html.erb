<!DOCTYPE html>
<html>
<head>
  <title>Jrc</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class='container'>
  <div class='row'>
  <% flash.each do |type, message| %>
    <div class='alert alert-<%= type %> fade in'>
      <%= message %>
    </div>
  <% end %>
  </div>
</div>

<%= yield %>

</body>
</html>
